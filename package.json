{
  "name": "psyche",
  "version": "0.0.1",
  "description": "A personalized external brain.",
  "scripts": {
    "lint": "tslint psyche/**/*.ts psyche/**/*.tsx",
    "start": "webpack-dev-server --open",
    "test": "npm run test:unit; npm run test:e2e",
    "test:e2e": "start-server-and-test 'npx webpack-dev-server' http://localhost:8080 'npx cypress run'",
    "test:e2e:watch": "start-server-and-test 'npx webpack-dev-server' http://localhost:8080 'npx cypress open'",
    "test:unit": "mocha 'psyche/**/*.test.ts' 'psyche/**/*.test.tsx'",
    "test:unit:watch": "npm run test:unit -- --watch --watch-extensions ts --watch-extensions tsx",
    "test:unit:coverage": "nyc npm run test:unit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/courageousillumination/psyche3.git"
  },
  "author": "Tristan Rasmussen",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/courageousillumination/psyche3/issues"
  },
  "homepage": "https://github.com/courageousillumination/psyche3#readme",
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^4.1.0",
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/chai-enzyme": "^0.6.6",
    "@types/chai": "^4.1.7",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/enzyme": "^3.9.3",
    "@types/mocha": "^5.2.7",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.1.0",
    "@types/react": "^16.8.20",
    "@types/sinon-chai": "^3.2.2",
    "@types/sinon": "^7.0.13",
    "chai-enzyme": "^1.0.0-beta.1",
    "chai": "^4.2.0",
    "cypress": "^3.3.1",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme": "^3.10.0",
    "html-webpack-plugin": "^3.2.0",
    "jsdom-global": "^3.0.2",
    "jsdom": "^15.1.1",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "sinon-chai": "^3.3.0",
    "sinon": "^7.3.2",
    "source-map-support": "^0.5.12",
    "start-server-and-test": "^1.9.1",
    "ts-loader": "^6.0.3",
